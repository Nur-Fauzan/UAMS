@{
    // View
    View = this;

    // Check action result
    if (participantsAdd.ActionResult is EmptyResult) // Terminate page
        return;

    // Layout
    Layout = participantsAdd.UseLayout ? "_Layout" : "";
}
<script>
var currentTable = @Html.Raw(ConvertToJson(participantsAdd.ToClientVar()));
ew.deepAssign(ew.vars, { tables: { Participants: currentTable } });
var currentPageID = ew.PAGE_ID = "add";
var currentForm;
var fParticipantsadd;
loadjs.ready(["wrapper", "head"], function () {
    let $ = jQuery;
    let fields = currentTable.fields;

    // Form object
    let form = new ew.FormBuilder()
        .setId("fParticipantsadd")
        .setPageId("add")

        // Add fields
        .setFields([

            ["UserId", [fields.UserId.visible && fields.UserId.required ? ew.Validators.required(fields.UserId.caption) : null, ew.Validators.integer], fields.UserId.isInvalid],
            ["AppointmentId", [fields.AppointmentId.visible && fields.AppointmentId.required ? ew.Validators.required(fields.AppointmentId.caption) : null, ew.Validators.integer], fields.AppointmentId.isInvalid],
            ["Status", [fields.Status.visible && fields.Status.required ? ew.Validators.required(fields.Status.caption) : null], fields.Status.isInvalid]
        ])

        // Form_CustomValidate
        .setCustomValidate(
            function(fobj) { // DO NOT CHANGE THIS LINE!
                    // Your custom validation code here, return false if invalid.
                    return true;
                }
        )

        // Use JavaScript validation or not
        .setValidateRequired(ew.CLIENT_VALIDATE)

        // Dynamic selection lists
        .setLists({
            "UserId": @Html.Raw(participantsAdd.UserId.ToClientList(participantsAdd)),
            "AppointmentId": @Html.Raw(participantsAdd.AppointmentId.ToClientList(participantsAdd)),
            "Status": @Html.Raw(participantsAdd.Status.ToClientList(participantsAdd)),
        })
        .build();
    window[form.id] = form;
    currentForm = form;
    loadjs.done(form.id);
});
</script>
<script>
loadjs.ready("head", function() {
    // Write your client script here, no need to add script tags.
});
</script>
@participantsAdd.ShowPageHeader()
@participantsAdd.ShowMessages()
<form name="fParticipantsadd" id="fParticipantsadd" class="@(participantsAdd.FormClassName)" action="@AppPath(CurrentPageName())" method="post" novalidate autocomplete="off">
@if (CurrentPage?.CheckToken ?? false) {
    <input type="hidden" name="@Config.TokenNameKey" value="@Config.TokenName"><!-- CSRF token name -->
    @Html.AntiForgeryToken()
}
<input type="hidden" name="action" id="action" value="insert">
<input type="hidden" name="modal" value="@ConvertToInt(participantsAdd.IsModal)">
@if (IsJsonResponse()) {
<input type="hidden" name="json" value="1">
}
<input type="hidden" name="@(participantsAdd.OldKeyName)" value="@Html.Raw(participantsAdd.OldKey)">
<div class="ew-add-div"><!-- page* -->
@if (participantsAdd.UserId.Visible) { // participantsAdd.UserId.Visible (begin)
    <div id="r_UserId" @Html.Raw(participantsAdd.UserId.RowAttributes)>
        <label id="elh_Participants_UserId" class="@(participantsAdd.LeftColumnClass)">@Html.Raw(participantsAdd.UserId.Caption)@Html.Raw(participantsAdd.UserId.Required ? Language.Phrase("FieldRequiredIndicator") : "")</label>
        <div class="@(participantsAdd.RightColumnClass)"><div @Html.Raw(participantsAdd.UserId.CellAttributes)>
<span id="el_Participants_UserId">
<span>@{
    if (IsRTL) {
        participantsAdd.UserId.EditAttrs["dir"] = "rtl";
    }
}</span>
<span id="as_x_UserId" class="ew-auto-suggest">
    <input type="text" class="form-control" name="sv_x_UserId" id="sv_x_UserId" value="@(RemoveHtml(ConvertToString(participantsAdd.UserId.EditValue)))" autocomplete="off" size="30" placeholder="@Html.Raw(HtmlEncode(participantsAdd.UserId.PlaceHolder))" data-placeholder="@Html.Raw(HtmlEncode(participantsAdd.UserId.PlaceHolder))" data-format-pattern="@(participantsAdd.UserId.FormatPattern)" @Html.Raw(participantsAdd.UserId.EditAttributes) aria-describedby="x_UserId_help">
</span>
<selection-list hidden class="form-control" data-table="Participants" data-field="x_UserId" data-input="sv_x_UserId" data-value-separator="@(participantsAdd.UserId.DisplayValueSeparatorAttribute)" name="x_UserId" id="x_UserId" value="@(HtmlEncode(participantsAdd.UserId.CurrentValue))"></selection-list>
@Html.Raw(participantsAdd.UserId.GetCustomMessage)
<div class="invalid-feedback">@Html.Raw(participantsAdd.UserId.GetErrorMessage())</div>
<script>
loadjs.ready(["fParticipantsadd"], function() {
    fParticipantsadd.createAutoSuggest(Object.assign({"id":"x_UserId","forceSelect":false}, { lookupAllDisplayFields: @(participantsAdd.UserId.Lookup.LookupAllDisplayFields ? "true" : "false") }, ew.vars.tables.Participants.fields.UserId.autoSuggestOptions));
});
</script>
@Html.Raw(participantsAdd.UserId.Lookup.GetParamTag(participantsAdd, "p_x_UserId"))
</span>
</div></div>
    </div>
} @* participantsAdd.UserId.Visible (end) *@
@if (participantsAdd.AppointmentId.Visible) { // participantsAdd.AppointmentId.Visible (begin)
    <div id="r_AppointmentId" @Html.Raw(participantsAdd.AppointmentId.RowAttributes)>
        <label id="elh_Participants_AppointmentId" class="@(participantsAdd.LeftColumnClass)">@Html.Raw(participantsAdd.AppointmentId.Caption)@Html.Raw(participantsAdd.AppointmentId.Required ? Language.Phrase("FieldRequiredIndicator") : "")</label>
        <div class="@(participantsAdd.RightColumnClass)"><div @Html.Raw(participantsAdd.AppointmentId.CellAttributes)>
<span id="el_Participants_AppointmentId">
<span>@{
    if (IsRTL) {
        participantsAdd.AppointmentId.EditAttrs["dir"] = "rtl";
    }
}</span>
<span id="as_x_AppointmentId" class="ew-auto-suggest">
    <input type="text" class="form-control" name="sv_x_AppointmentId" id="sv_x_AppointmentId" value="@(RemoveHtml(ConvertToString(participantsAdd.AppointmentId.EditValue)))" autocomplete="off" size="30" placeholder="@Html.Raw(HtmlEncode(participantsAdd.AppointmentId.PlaceHolder))" data-placeholder="@Html.Raw(HtmlEncode(participantsAdd.AppointmentId.PlaceHolder))" data-format-pattern="@(participantsAdd.AppointmentId.FormatPattern)" @Html.Raw(participantsAdd.AppointmentId.EditAttributes) aria-describedby="x_AppointmentId_help">
</span>
<selection-list hidden class="form-control" data-table="Participants" data-field="x_AppointmentId" data-input="sv_x_AppointmentId" data-value-separator="@(participantsAdd.AppointmentId.DisplayValueSeparatorAttribute)" name="x_AppointmentId" id="x_AppointmentId" value="@(HtmlEncode(participantsAdd.AppointmentId.CurrentValue))"></selection-list>
@Html.Raw(participantsAdd.AppointmentId.GetCustomMessage)
<div class="invalid-feedback">@Html.Raw(participantsAdd.AppointmentId.GetErrorMessage())</div>
<script>
loadjs.ready(["fParticipantsadd"], function() {
    fParticipantsadd.createAutoSuggest(Object.assign({"id":"x_AppointmentId","forceSelect":false}, { lookupAllDisplayFields: @(participantsAdd.AppointmentId.Lookup.LookupAllDisplayFields ? "true" : "false") }, ew.vars.tables.Participants.fields.AppointmentId.autoSuggestOptions));
});
</script>
@Html.Raw(participantsAdd.AppointmentId.Lookup.GetParamTag(participantsAdd, "p_x_AppointmentId"))
</span>
</div></div>
    </div>
} @* participantsAdd.AppointmentId.Visible (end) *@
@if (participantsAdd.Status.Visible) { // participantsAdd.Status.Visible (begin)
    <div id="r_Status" @Html.Raw(participantsAdd.Status.RowAttributes)>
        <label id="elh_Participants_Status" for="x_Status" class="@(participantsAdd.LeftColumnClass)">@Html.Raw(participantsAdd.Status.Caption)@Html.Raw(participantsAdd.Status.Required ? Language.Phrase("FieldRequiredIndicator") : "")</label>
        <div class="@(participantsAdd.RightColumnClass)"><div @Html.Raw(participantsAdd.Status.CellAttributes)>
<span id="el_Participants_Status">
    <select
        id="x_Status"
        name="x_Status"
        class="form-select ew-select@(participantsAdd.Status.IsInvalidClass)"
        data-select2-id="fParticipantsadd_x_Status"
        data-table="Participants"
        data-field="x_Status"
        data-value-separator="@(participantsAdd.Status.DisplayValueSeparatorAttribute)"
        data-placeholder="@(HtmlEncode(participantsAdd.Status.PlaceHolder))"
        @Html.Raw(participantsAdd.Status.EditAttributes)>
        @(participantsAdd.Status.SelectOptionListHtml("x_Status"))
    </select>
    @Html.Raw(participantsAdd.Status.GetCustomMessage)
    <div class="invalid-feedback">@Html.Raw(participantsAdd.Status.GetErrorMessage())</div>
<script>
loadjs.ready("fParticipantsadd", function() {
    var options = { name: "x_Status", selectId: "fParticipantsadd_x_Status" },
        el = document.querySelector("select[data-select2-id='" + options.selectId + "']");
    if (!el)
        return;
    options.closeOnSelect = !options.multiple;
    options.dropdownParent = el.closest("#ew-modal-dialog, #ew-add-opt-dialog");
    if (fParticipantsadd.lists.Status?.lookupOptions.length) {
        options.data = { id: "x_Status", form: "fParticipantsadd" };
    } else {
        options.ajax = { id: "x_Status", form: "fParticipantsadd", limit: ew.LOOKUP_PAGE_SIZE };
    }
    options.minimumResultsForSearch = Infinity;
    options = Object.assign({}, ew.selectOptions, options, ew.vars.tables.Participants.fields.Status.selectOptions);
    ew.createSelect(options);
});
</script>
</span>
</div></div>
    </div>
} @* participantsAdd.Status.Visible (end) *@
</div><!-- /page* -->
@{ participantsAdd.CloseRecordset(); }
@Html.Raw(participantsAdd.IsModal ? "<template class=\"ew-modal-buttons\">" : "<div class=\"row ew-buttons\">")<!-- buttons .row -->
    <div class="@(participantsAdd.OffsetColumnClass)"><!-- buttons offset -->
    <button class="btn btn-primary ew-btn" name="btn-action" id="btn-action" type="submit" form="fParticipantsadd">@Html.Raw(Language.Phrase("AddBtn"))</button>
@if (IsJsonResponse()) {
    <button class="btn btn-default ew-btn" name="btn-cancel" id="btn-cancel" type="button" data-bs-dismiss="modal">@Html.Raw(Language.Phrase("CancelBtn"))</button>
} else {
    <button class="btn btn-default ew-btn" name="btn-cancel" id="btn-cancel" type="button" form="fParticipantsadd" data-href="@AppPath(participantsAdd.ReturnUrl)">@Html.Raw(Language.Phrase("CancelBtn"))</button>
}
    </div><!-- /buttons offset -->
@Html.Raw(participantsAdd.IsModal ? "</template>" : "</div>")<!-- /buttons .row -->
</form>
@participantsAdd.ShowPageFooter()
@Html.Raw(GetDebugMessage())
@ElapsedTime()
<script>
// Field event handlers
loadjs.ready("head", function() {
    ew.addEventHandlers("Participants");
});
</script>
